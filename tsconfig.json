{
"compilerOptions": {
  "target": "ESNext",
  "useUnknownInCatchVariables": false,
  "module": "ESNext",
  "lib": ["DOM", "ESNext"],
  "jsx": "react-jsx",
  "moduleResolution": "bundler",
  "strict": true,
  "esModuleInterop": true,
  "skipLibCheck": true,
  "forceConsistentCasingInFileNames": true
  }
}